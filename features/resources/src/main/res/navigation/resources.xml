<?xml version="1.0" encoding="utf-8"?>
<navigation
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/resources"
    app:startDestination="@id/resourceDetails">

    <fragment
        android:id="@+id/resourceDetails"
        android:name="com.passbolt.mobile.android.feature.resources.details.ResourceDetailsFragment"
        android:label="ResourceDetailsFragment"
        tools:layout="@layout/fragment_resource_details">
        <argument
            android:name="resourceModel"
            app:argType="com.passbolt.mobile.android.ui.ResourceModel" />
    </fragment>

    <fragment
        android:id="@+id/newResource"
        android:name="com.passbolt.mobile.android.feature.resources.new.NewResourceFragment"
        android:label="NewResourceFragment"
        tools:layout="@layout/fragment_new_resource" />

</navigation>
