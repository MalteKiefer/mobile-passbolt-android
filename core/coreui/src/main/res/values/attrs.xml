<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TextInputView">
        <attr name="inputTitle"
            format="string" />
        <attr name="inputHint"
            format="string" />
        <attr name="inputIsRequired"
            format="boolean" />
    </declare-styleable>
    <declare-styleable name="ResultView">
        <attr name="resultTitle"
            format="string" />
        <attr name="resultDescription"
            format="string" />
        <attr name="resultIcon"
            format="reference" />
    </declare-styleable>
</resources>
